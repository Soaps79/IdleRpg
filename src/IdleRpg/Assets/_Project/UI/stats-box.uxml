<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/_Project/UI/stats-box-style.uss?fileID=7433441132597879392&amp;guid=372a66f9a3837c64ca3de42681d9f0a4&amp;type=3#stats-box-style" />
